        "lib": [
            "es6"
        ],
        "sourceMap": true,
        "allowUnusedLabels": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "rootDir": "./src"
    },
    "exclude": [
        "node_modules",
        "out",
        ".vscode-test"
    ]
